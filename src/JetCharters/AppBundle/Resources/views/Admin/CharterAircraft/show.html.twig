{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CharterAircraft</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Operatorid</th>
                <td>{{ entity.operatorID }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Seats</th>
                <td>{{ entity.seats }}</td>
            </tr>
            <tr>
                <th>Tailnumber</th>
                <td>{{ entity.tailNumber }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ entity.location }}</td>
            </tr>
            <tr>
                <th>Availablefrom</th>
                <td>{{ entity.availableFrom|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Availableto</th>
                <td>{{ entity.availableTo|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Photos</th>
                <td>{{ entity.photos }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Video</th>
                <td>{{ entity.video }}</td>
            </tr>
            <tr>
                <th>Hourlyrate</th>
                <td>{{ entity.hourlyRate }}</td>
            </tr>
            <tr>
                <th>Hourlyrate2</th>
                <td>{{ entity.hourlyRate2 }}</td>
            </tr>
            <tr>
                <th>Airambulance</th>
                <td>{{ entity.airAmbulance }}</td>
            </tr>
            <tr>
                <th>Hidetailnumber</th>
                <td>{{ entity.hideTailNumber }}</td>
            </tr>
            <tr>
                <th>Realtailnumber</th>
                <td>{{ entity.realTailNumber }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('operator_aircraft') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('operator_aircraft_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
